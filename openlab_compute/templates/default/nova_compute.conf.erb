[DEFAULT]
dhcpbridge_flagfile=/etc/nova/nova.conf
dhcpbridge=/usr/bin/nova-dhcpbridge
#logdir=/var/log/nova
state_path=/var/lib/nova
lock_path=/var/lock/nova
force_dhcp_release=True
libvirt_use_virtio_for_bridges=True
verbose=True
ec2_private_dns_show_ip=True
api_paste_config=/etc/nova/api-paste.ini
enabled_apis=ec2,osapi_compute,metadata
transport_url = rabbit://openstack:<%= "#{@rabbit_pass}"  %>@openlab-controller
auth_strategy = keystone
my_ip = 128.194.142.171
use_neutron = True
firewall_driver = nova.virt.firewall.NoopFirewallDriver

[keystone_authtoken]
auth_uri = http://openlab-controller:5000
auth_url = http://openlab-controller:35357
memcached_servers = openlab-controller:11211
auth_type = password
project_domain_name = default
user_domain_name = default
project_name = service
username = nova
password = <%= "#{@nova_user_pass}"  %>


[vnc]
enabled = True
vncserver_listen = 0.0.0.0
vncserver_proxyclient_address = $my_ip
novncproxy_base_url = http://128.194.142.141:6080/vnc_auto.html

[glance]
api_servers = http://openlab-controller:9292

[oslo_concurrency]
lock_path = /var/lib/nova/tmp

[neutron]
url = http://openlab-controller:9696
auth_url = http://openlab-controller:35357
auth_type = password
project_domain_name = default
user_domain_name = default
region_name = RegionOne
project_name = service
username = neutron
password = <%= "#{@neutron_user_pass}"  %>







